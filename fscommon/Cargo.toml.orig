[package]
name = "fscommon"
version = "0.1.1"
authors = ["Rafał Harabień <rafalh1992@o2.pl>"]
repository = "https://github.com/rafalh/rust-fscommon"
readme = "README.md"
keywords = ["filesystem", "no_std"]
categories = ["filesystem"]
license = "MIT"
description = """
Filesystem common utilities.
"""

[features]
# Use Rust std library
std = []
# Default features
default = ["std"]

[dependencies]
core_io = { version = "0.1", optional = true }
log = "0.4"

[dev-dependencies]
env_logger = "0.5"
